{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.configPath": "./prettier.config.mjs",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "xml",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "defu",
    "Destructurable",
    "dtrace",
    "esbuild",
    "estree",
    "iconify",
    "INTLIFY",
    "lezer",
    "lucide",
    "nocheck",
    "ofetch",
    "oxlint",
    "pinia",
    "sxzz",
    "taplo",
    "unann",
    "unplugin",
    "unrs",
    "valibot",
    "vercel",
    "vueuse"
  ]
}
