{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "vue",
    "yaml",
    "xml",
    "html",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "defu",
    "Destructurable",
    "estree",
    "iconify",
    "INTLIFY",
    "lezer",
    "nocheck",
    "ofetch",
    "pinia",
    "sxzz",
    "unann",
    "unplugin",
    "valibot",
    "vercel",
    "vueuse"
  ]
}
