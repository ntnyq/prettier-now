{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.22",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "description": "Yet another Prettier browser extension for code formatting.",
  "keywords": [
    "browser-extension",
    "prettier"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/prettier-now",
  "repository": "ntnyq/prettier-now",
  "bugs": {
    "url": "https://github.com/ntnyq/prettier-now/issues"
  },
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --build",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-angular": "^0.1.4",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.4.0",
    "@codemirror/lang-php": "^6.0.2",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/legacy-modes": "^6.5.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@iconify-json/lucide": "^1.2.70",
    "@iconify-json/ri": "^1.2.6",
    "@iconify-json/vscode-icons": "^1.2.32",
    "@lezer/highlight": "^1.2.2",
    "@ntnyq/utils": "^0.9.2",
    "@prettier/plugin-php": "^0.24.0",
    "@prettier/plugin-pug": "^3.4.2",
    "@prettier/plugin-xml": "^3.4.2",
    "@replit/codemirror-lang-svelte": "^6.0.0",
    "@uiw/codemirror-themes": "^4.25.2",
    "@unocss/reset": "^66.5.4",
    "@vueuse/core": "^13.9.0",
    "cm6-graphql": "^0.2.1",
    "codemirror": "^6.0.2",
    "dayjs": "^1.11.18",
    "floating-vue": "^5.2.2",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.6",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-toml": "2.0.2",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.22",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.6.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/i18n": "^0.2.4",
    "@wxt-dev/module-vue": "^1.0.3",
    "@wxt-dev/unocss": "^1.0.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.9.3",
    "unocss": "^66.5.4",
    "unplugin-vue-components": "^29.2.0",
    "vite": "^7.1.11",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue-tsc": "^3.1.1",
    "wxt": "^0.20.11"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
