{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.8",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "description": "Yet another Prettier browser extension for formatting js, css, html, json codes",
  "license": "MIT",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint .",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.2.5",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-yaml": "^6.1.1",
    "@codemirror/language": "^6.10.2",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.27.0",
    "@iconify/json": "^2.2.217",
    "@lezer/highlight": "^1.2.0",
    "@uiw/codemirror-themes": "^4.22.1",
    "@unocss/reset": "^0.60.4",
    "@vueuse/core": "^10.10.0",
    "codemirror": "^6.0.1",
    "copy-text-to-clipboard": "^3.2.0",
    "dayjs": "^1.11.11",
    "floating-vue": "^5.2.2",
    "pinia": "^2.1.7",
    "prettier": "^3.3.1",
    "type-fest": "^4.19.0",
    "vercel-toast": "^1.8.0",
    "vue": "^3.4.27",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.3.2",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^2.0.1",
    "@ntnyq/prettier-config": "^1.21.2",
    "@unocss/eslint-config": "^0.60.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "bumpp": "^9.4.1",
    "eslint": "^9.4.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.0",
    "tsx": "^4.12.0",
    "typescript": "^5.4.5",
    "unocss": "^0.60.4",
    "unplugin-vue-components": "^0.27.0",
    "vue-tsc": "^2.0.19",
    "wxt": "^0.18.4"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,vue,md,json,yml,yaml}": "eslint --fix"
  }
}
