{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.18",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "description": "Yet another Prettier browser extension for formatting js, css, html, json codes",
  "license": "MIT",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint .",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-angular": "^0.1.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.1",
    "@codemirror/lang-php": "^6.0.1",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.10.8",
    "@codemirror/state": "^6.5.0",
    "@codemirror/view": "^6.36.1",
    "@iconify-json/lucide": "^1.2.20",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/vscode-icons": "^1.2.6",
    "@lezer/highlight": "^1.2.1",
    "@prettier/plugin-php": "^0.22.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@replit/codemirror-lang-svelte": "^6.0.0",
    "@uiw/codemirror-themes": "^4.23.7",
    "@unocss/reset": "^0.65.3",
    "@vueuse/core": "^12.2.0",
    "cm6-graphql": "^0.2.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "petite-vue-i18n": "^11.0.1",
    "pinia": "^2.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-java": "^2.6.5",
    "prettier-plugin-svelte": "^3.3.2",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.13",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.10.4",
    "@ntnyq/prettier-config": "^1.22.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unocss": "^0.65.3",
    "unplugin-vue-components": "^0.28.0",
    "vue-tsc": "^2.2.0",
    "wxt": "^0.19.23"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,vue,html,md,json,yml,yaml}": "eslint --fix"
  }
}
