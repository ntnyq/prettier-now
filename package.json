{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.10",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "description": "Yet another Prettier browser extension for formatting js, css, html, json codes",
  "license": "MIT",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint .",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.0",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.0",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-yaml": "^6.1.1",
    "@codemirror/language": "^6.10.3",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.1",
    "@iconify/json": "^2.2.258",
    "@lezer/highlight": "^1.2.1",
    "@uiw/codemirror-themes": "^4.23.5",
    "@unocss/reset": "^0.63.4",
    "@vueuse/core": "^11.1.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "petite-vue-i18n": "^10.0.4",
    "pinia": "^2.2.4",
    "prettier": "^3.3.3",
    "type-fest": "^4.26.1",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.11",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.4.5",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.0.0-beta.19",
    "@ntnyq/prettier-config": "^1.21.3",
    "@unocss/eslint-config": "^0.63.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "unocss": "^0.63.4",
    "unplugin-vue-components": "^0.27.4",
    "vue-tsc": "^2.1.6",
    "wxt": "^0.19.11"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,cjs,mjs,vue,html,md,json,yml,yaml}": "eslint --fix"
  }
}
