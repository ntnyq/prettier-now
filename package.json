{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.20",
  "private": true,
  "packageManager": "pnpm@10.3.0",
  "description": "Yet another Prettier browser extension for formatting js, css, html, json codes",
  "license": "MIT",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint .",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-angular": "^0.1.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/lang-php": "^6.0.1",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.10.8",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.2",
    "@iconify-json/lucide": "^1.2.26",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/vscode-icons": "^1.2.14",
    "@lezer/highlight": "^1.2.1",
    "@ntnyq/utils": "^0.5.2",
    "@prettier/plugin-php": "^0.22.4",
    "@prettier/plugin-xml": "^3.4.1",
    "@replit/codemirror-lang-svelte": "^6.0.0",
    "@uiw/codemirror-themes": "^4.23.8",
    "@unocss/reset": "^65.4.3",
    "@vueuse/core": "^12.5.0",
    "cm6-graphql": "^0.2.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "pinia": "^3.0.1",
    "prettier": "^3.5.1",
    "prettier-plugin-java": "^2.6.7",
    "prettier-plugin-svelte": "^3.3.3",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.13",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.4",
    "@ntnyq/prettier-config": "^2.0.0-beta.2",
    "@types/chrome": "^0.0.304",
    "@vitejs/plugin-vue": "^5.2.1",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/i18n": "^0.2.3",
    "@wxt-dev/module-vue": "^1.0.2",
    "@wxt-dev/unocss": "^1.0.0",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unplugin-vue-components": "^28.0.0",
    "vue-tsc": "^2.2.0",
    "wxt": "^0.19.27"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "dtrace-provider",
      "esbuild",
      "sharp",
      "spawn-sync"
    ]
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,vue,svg,md,json,yml,yaml}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
