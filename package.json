{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "description": "Yet another Prettier browser extension for formatting js, css, html, json codes",
  "license": "MIT",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint .",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.8",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.2.4",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-yaml": "^6.0.0",
    "@codemirror/language": "^6.10.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.0",
    "@iconify/json": "^2.2.196",
    "@lezer/highlight": "^1.2.0",
    "@uiw/codemirror-themes": "^4.21.25",
    "@unocss/reset": "^0.58.6",
    "@vueuse/core": "^10.9.0",
    "codemirror": "^6.0.1",
    "copy-text-to-clipboard": "^3.2.0",
    "dayjs": "^1.11.10",
    "defu": "^6.1.4",
    "floating-vue": "^5.2.2",
    "ofetch": "^1.3.4",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "type-fest": "^4.14.0",
    "valibot": "^0.30.0",
    "vercel-toast": "^1.8.0",
    "vue": "^3.4.21",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.3.0",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.21.0",
    "@ntnyq/prettier-config": "^1.21.0",
    "@unocss/eslint-config": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "unocss": "^0.58.6",
    "unplugin-vue-components": "^0.26.0",
    "vue-tsc": "^2.0.7",
    "wxt": "^0.17.9"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,vue,md,json,yml,yaml}": "eslint --fix"
  }
}
