{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.21",
  "private": true,
  "packageManager": "pnpm@10.7.0",
  "description": "Yet another Prettier browser extension for code formatting.",
  "keywords": [
    "browser-extension",
    "prettier"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/prettier-now",
  "repository": "ntnyq/prettier-now",
  "bugs": {
    "url": "https://github.com/ntnyq/prettier-now/issues"
  },
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "vue-tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "lint": "eslint",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@codemirror/lang-angular": "^0.1.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/lang-php": "^6.0.1",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/lang-vue": "^0.1.3",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.0",
    "@codemirror/legacy-modes": "^6.5.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.5",
    "@iconify-json/lucide": "^1.2.34",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/vscode-icons": "^1.2.18",
    "@lezer/highlight": "^1.2.1",
    "@ntnyq/utils": "^0.6.2",
    "@prettier/plugin-php": "^0.22.4",
    "@prettier/plugin-pug": "^3.2.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@replit/codemirror-lang-svelte": "^6.0.0",
    "@uiw/codemirror-themes": "^4.23.10",
    "@unocss/reset": "^66.1.0-beta.8",
    "@vueuse/core": "^13.0.0",
    "cm6-graphql": "^0.2.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "pinia": "^3.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-java": "^2.6.7",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-toml": "^2.0.3",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.13",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "4.0.2",
    "@ntnyq/prettier-config": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/i18n": "^0.2.3",
    "@wxt-dev/module-vue": "^1.0.2",
    "@wxt-dev/unocss": "^1.0.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "typescript": "^5.8.2",
    "unocss": "^66.1.0-beta.8",
    "unplugin-vue-components": "^28.4.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vue-tsc": "^2.2.8",
    "wxt": "^0.20.0"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
