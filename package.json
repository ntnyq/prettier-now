{
  "name": "prettier-now",
  "type": "module",
  "version": "0.0.23",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "description": "Yet another Prettier browser extension for code formatting.",
  "keywords": [
    "browser-extension",
    "prettier"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/prettier-now",
  "repository": "ntnyq/prettier-now",
  "bugs": {
    "url": "https://github.com/ntnyq/prettier-now/issues"
  },
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "lint": "eslint",
    "postinstall": "wxt prepare",
    "prepare": "husky",
    "release": "bumpp",
    "release:check": "run-s format:check lint typecheck",
    "typecheck": "vue-tsc --build",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.89",
    "@iconify-json/ri": "^1.2.9",
    "@iconify-json/vscode-icons": "^1.2.40",
    "@ntnyq/utils": "^0.11.0",
    "@prettier/plugin-php": "^0.24.0",
    "@prettier/plugin-pug": "^3.4.2",
    "@prettier/plugin-xml": "^3.4.2",
    "@unocss/reset": "^66.6.0",
    "@vueuse/core": "^14.2.0",
    "dayjs": "^1.11.19",
    "floating-vue": "^5.2.2",
    "monaco-editor": "^0.55.1",
    "pinia": "^3.0.4",
    "prettier": "^3.8.1",
    "prettier-plugin-java": "^2.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-toml": "2.0.2",
    "vercel-toast": "^1.8.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^6.0.0-beta.7",
    "@typescript/native-preview": "^7.0.0-dev.20260206.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/i18n": "^0.2.4",
    "@wxt-dev/module-vue": "^1.0.3",
    "@wxt-dev/unocss": "^1.0.1",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.28.0",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vue-tsc": "^3.2.4",
    "wxt": "^0.20.14"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*": "oxfmt --no-error-on-unmatched-pattern"
  }
}
