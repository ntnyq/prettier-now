{
  "root": true,
  "extends": ["@ntnyq", "@unocss", "./.wxt/eslintrc-auto-import.json"],
  "rules": {
    "vue/html-self-closing": [
      "error",
      {
        "html": {
          "void": "always",
          "normal": "always",
          "component": "always"
        },
        "svg": "always",
        "math": "always"
      }
    ]
  }
}
